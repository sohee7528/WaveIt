<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Notification">

    <ImageButton
        android:id="@+id/back5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="43dp"
        android:backgroundTint="@color/white"
        android:src="@drawable/back"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck" />

    <ImageView
        android:id="@+id/textnotification"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="1dp"
        android:layout_marginBottom="11dp"
        android:src="@drawable/textnotification"
        app:layout_constraintBottom_toTopOf="@+id/leftnotificaion"
        app:layout_constraintStart_toStartOf="@+id/leftnotificaion" />

    <TextView
        android:id="@+id/leftnotificaion"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="150dp"
        android:text="1개 알림 남음"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/noticegroup"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="23dp"
        android:layout_marginBottom="19dp"
        android:backgroundTint="@color/white"
        android:src="@drawable/noticegroup"
        app:layout_constraintBottom_toTopOf="@+id/takecontracetive1"
        app:layout_constraintEnd_toEndOf="parent"
        tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

    <ImageButton
        android:id="@+id/takecontracetive1"
        android:layout_width="0dp"
        android:layout_height="91dp"
        android:layout_marginTop="75dp"
        android:layout_marginBottom="89dp"
        android:backgroundTint="#EBE0D8"
        android:src="@drawable/take_contraceptive"
        app:layout_constraintBottom_toBottomOf="@+id/changepad1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textnotification"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/changepad1"
        android:layout_width="0dp"
        android:layout_height="90dp"
        android:layout_marginTop="286dp"
        android:backgroundTint="#ffffff"
        android:src="@drawable/change_pad"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/changetampon1"
        android:layout_width="0dp"
        android:layout_height="90dp"
        android:layout_marginTop="90dp"
        android:layout_marginBottom="90dp"
        android:backgroundTint="#ffffff"
        android:src="@drawable/change_tampon"
        app:layout_constraintBottom_toBottomOf="@+id/takecontracetive2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/changepad1"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/takecontracetive2"
        android:layout_width="0dp"
        android:layout_height="90dp"
        android:layout_marginTop="90dp"
        android:layout_marginBottom="90dp"
        android:backgroundTint="#ffffff"
        android:src="@drawable/take_contraceptive"
        app:layout_constraintBottom_toBottomOf="@+id/changepad2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/changepad1"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/changepad2"
        android:layout_width="0dp"
        android:layout_height="90dp"
        android:layout_marginTop="180dp"
        android:layout_marginBottom="180dp"
        android:backgroundTint="#ffffff"
        android:src="@drawable/change_pad"
        app:layout_constraintBottom_toBottomOf="@+id/takecontracetive3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/changepad1"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/changetampon2"
        android:layout_width="0dp"
        android:layout_height="90dp"
        android:layout_marginTop="90dp"
        android:layout_marginBottom="90dp"
        android:backgroundTint="#ffffff"
        android:src="@drawable/change_tampon"
        app:layout_constraintBottom_toBottomOf="@+id/takecontracetive3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/changepad2"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/takecontracetive3"
        android:layout_width="393dp"
        android:layout_height="90dp"
        android:layout_marginTop="736dp"
        android:backgroundTint="#ffffff"
        android:src="@drawable/take_contraceptive"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="SpeakableTextPresentCheck" />

</androidx.constraintlayout.widget.ConstraintLayout>